{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/src/index.ts",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy",
        "--extensions",
        "\".ts\""
      ],
      "env": {
        "BABEL_ENV": "development"
      },
      "envFile": "${workspaceFolder}/.env",
    }
  ]
}